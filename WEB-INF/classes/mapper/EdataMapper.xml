<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stand.spring.dao.EdataMapper">
    <resultMap id="BaseResultMap" type="com.stand.spring.model.EdataDO">
        <id column="data_id" jdbcType="INTEGER" property="dataId"/>
        <result column="data_time" jdbcType="TIMESTAMP" property="dataTime"/>
        <!--<result column="data_state" jdbcType="BIT" property="dataState"/>-->
        <result column="data_vals" jdbcType="VARCHAR" property="dataVals"/>
    </resultMap>

    <select id="selectEdata" parameterType="java.util.Map" resultMap="BaseResultMap">
      select
        data_vals,data_time
      from
        ${table}
      where
        data_time
      BETWEEN
        #{startDate}
      and
        #{endDate}
    </select>
</mapper>